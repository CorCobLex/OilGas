<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:oilGas="clr-namespace:OilGas;assembly=OilGas"
             x:Class="OilGas.NewElementPage"
             x:DataType="oilGas:NewElementViewModel">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Название"
                   Text="{Binding Name, Mode=TwoWay}"/>
            <Entry Placeholder="Инвентарный номер"
                   Text="{Binding InventoryNumber, Mode=TwoWay}"/>
            <Entry Placeholder="Код"
                   Text="{Binding Code, Mode=TwoWay}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>